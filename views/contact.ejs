<%- include('partials/nav.ejs') %>
    <h1>Contact-us</h1>
    <div class="Contact">
        <form action="/contact" method="post">
            <label for="name">Name</label><br>
            <input type="text" name="name" id="name" value=<%=data.name %> ><br>
            <label for="age">Age</label><br />
            <input type="number" name="age" id="age" value=<%=data.age %> ><br>
            <label for="mobileNo">Mobile Number</label><br>
            <input type="number" name="mobileNo" id="mobileNo" value=<%=data.mobile %> ><br>
            <button type="submit">Submit</button>
        </form>
    </div>
    <%- include('partials/footer.ejs')%>